<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" android:alpha="0.9">
    <solid android:color="#FFFFFF" />
    <corners android:radius="20dp" />
    <gradient
        android:angle="45"
        android:centerX="50%"
        android:centerColor="#FFFF0F"
        android:startColor="#02FF02"
        android:endColor="#FFA500"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="250dp"
        android:height="100dp"
        />
    <stroke
        android:width="3dp"
        android:color="#292929"
        />


</shape>


